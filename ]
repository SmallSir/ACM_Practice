#include<iostream>  
#include<cstdio>
#include<stdio.h>
#include<cstring>
#include<cstdio>
#include<climits>   
#include<cmath> 
#include<vector>  
#include <bitset>  
#include<algorithm>    
#include <queue>  
#include<map> 
#define inf 9999999; 
using namespace std;


int a[1000005],b[1000005];
int n,i,sum,m,x;
int main()
{
	cin>>n;
	for(i=1;i<=n;i++)
	{
		cin>>a[i];
		b[i]=a[i];
	}
	cin>>m;
	for(i=1;i<=m;i++)
	{
		cin>>x;
		a[x]=-1;
	}
	sum=0;
	for(i=1;i<=n;i++)
		sum+=a[i];
	if(sum<0)
	{
		cout<<"NO"<<endl;
		return 0;
	}
	bool flag=0;
	//cout<<"sum "<<sum<<endl;
	for(i=1;i<=n;i++)
	{
		if(a[i]>0)
		{
			if(sum<a[i])
			{
				//cout<<a[i]<<endl;
				a[i]-=sum;
				sum=0;
				//cout<<"2 "<<i<<" "<<sum<<" "<<a[i]<<endl;
			}
			else if(sum>a[i])
			{
				sum-=a[i]*2;
				a[i]=-a[i];
				//cout<<"1 "<<i<<" "<<sum<<endl;
			}
		}
		else
		{
			if(sum<=b[i])
			{
				b[i]=-sum;
				sum=0;
			}
			else
			{
				sum-=b[i];
				sum++;
				a[i]=-b[i];
			}
		}
		if(sum==0)
		{
			flag=1;
			break;
		}
	}
	if(flag==1)
	{
		cout<<"YES"<<endl;
		for(i=1;i<n;i++)
		{
			cout<<a[i]<<" ";
		}
		cout<<a[i]<<endl;
	}
	else
	{
		cout<<"NO"<<endl;
	}
}
